{
  "openapi": "3.0.0",
  "info": {
    "title": "backend",
    "version": "0.1.0"
  },
  "paths": {
    "/login": {
      "get": {
        "tags": [
          "Login"
        ],
        "description": "Check login status",
        "operationId": "get_login",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "default": {
            "description": ""
          }
        }
      },
      "post": {
        "tags": [
          "Login"
        ],
        "description": "Login with email and password",
        "operationId": "post_login",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Login"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponse"
                }
              }
            }
          },
          "default": {
            "description": ""
          }
        }
      }
    },
    "/register": {
      "post": {
        "tags": [
          "Login"
        ],
        "description": "Register with first name, last name, email and password.\n\nFirst user is automatically an admin",
        "operationId": "post_register",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Register"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponse"
                }
              }
            }
          },
          "default": {
            "description": ""
          }
        }
      }
    },
    "/bookings": {
      "get": {
        "tags": [
          "Bookings"
        ],
        "description": "Only accessible by authenticated users\n\nUsers can only see their own bookings\n\nAdmins can see all bookings",
        "operationId": "get_bookings",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/SerializeBooking"
                  }
                }
              }
            }
          },
          "default": {
            "description": ""
          }
        }
      },
      "post": {
        "tags": [
          "Bookings"
        ],
        "description": "Only accessible by authenticated users",
        "operationId": "post_bookings",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PostBooking"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SerializeBooking"
                }
              }
            }
          },
          "default": {
            "description": ""
          }
        }
      }
    },
    "/bookings/{id}": {
      "delete": {
        "tags": [
          "Bookings"
        ],
        "description": "Users can only delete their own bookings\n\nAdmins can delete any booking",
        "operationId": "delete_bookings",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "default": {
            "description": ""
          }
        }
      },
      "patch": {
        "tags": [
          "Bookings"
        ],
        "description": "Only accessible by admins\n\nSet reason, date, status to empty string to leave unchanged\n\nSet duration to -1 to leave unchanged",
        "operationId": "patch_bookings",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateBooking"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SerializeBooking"
                }
              }
            }
          },
          "default": {
            "description": ""
          }
        }
      }
    },
    "/users": {
      "get": {
        "tags": [
          "Users"
        ],
        "description": "Only accessible by admins",
        "operationId": "get_users",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            }
          },
          "default": {
            "description": ""
          }
        }
      },
      "post": {
        "tags": [
          "Users"
        ],
        "description": "Only accessible by admins",
        "operationId": "post_users",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PostUser"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "default": {
            "description": ""
          }
        }
      }
    },
    "/users/{id}": {
      "put": {
        "tags": [
          "Users"
        ],
        "description": "Only accessible by admins",
        "operationId": "put_users",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PutUser"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "default": {
            "description": ""
          }
        }
      },
      "delete": {
        "tags": [
          "Users"
        ],
        "description": "Only accessible by admins",
        "operationId": "delete_users",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    },
    "/rooms": {
      "get": {
        "tags": [
          "Rooms"
        ],
        "description": "Only accessible by authenticated users",
        "operationId": "get_rooms",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/RoomResponse"
                  }
                }
              }
            }
          },
          "default": {
            "description": ""
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginResponse": {
        "type": "object",
        "required": [
          "token",
          "user"
        ],
        "properties": {
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "token": {
            "type": "string"
          }
        }
      },
      "User": {
        "type": "object",
        "required": [
          "email",
          "first_name",
          "last_name",
          "password"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "is_admin": {
            "type": "boolean",
            "nullable": true
          },
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "created_at": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "Login": {
        "type": "object",
        "required": [
          "email",
          "password"
        ],
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "Register": {
        "type": "object",
        "required": [
          "email",
          "first_name",
          "last_name",
          "password"
        ],
        "properties": {
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "SerializeBooking": {
        "type": "object",
        "required": [
          "date",
          "duration",
          "reason",
          "room",
          "status",
          "user"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "reason": {
            "type": "string"
          },
          "duration": {
            "description": "0 = Morning, 1 = Afternoon, 2 = Day",
            "type": "integer",
            "format": "int32"
          },
          "status": {
            "description": "Pending, Rejected, Approved",
            "type": "string"
          },
          "date": {
            "description": "YYYY-MM-DD",
            "type": "string"
          },
          "room": {
            "$ref": "#/components/schemas/Room"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "created_at": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "Room": {
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "name": {
            "type": "string"
          }
        }
      },
      "PostBooking": {
        "type": "object",
        "required": [
          "date",
          "duration",
          "reason",
          "room_id"
        ],
        "properties": {
          "reason": {
            "type": "string"
          },
          "duration": {
            "description": "0 = Morning, 1 = Afternoon, 2 = Day",
            "type": "integer",
            "format": "int32"
          },
          "date": {
            "description": "YYYY-MM-DD",
            "type": "string"
          },
          "room_id": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "UpdateBooking": {
        "type": "object",
        "properties": {
          "reason": {
            "type": "string",
            "nullable": true
          },
          "duration": {
            "description": "0 = Morning, 1 = Afternoon, 2 = Day",
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "status": {
            "description": "Pending, Rejected, Approved",
            "type": "string",
            "nullable": true
          },
          "date": {
            "description": "YYYY-MM-DD",
            "type": "string",
            "nullable": true
          }
        }
      },
      "PostUser": {
        "type": "object",
        "required": [
          "email",
          "first_name",
          "is_admin",
          "last_name",
          "password"
        ],
        "properties": {
          "is_admin": {
            "type": "boolean"
          },
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "PutUser": {
        "type": "object",
        "required": [
          "email",
          "first_name",
          "is_admin",
          "last_name"
        ],
        "properties": {
          "is_admin": {
            "type": "boolean"
          },
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          }
        }
      },
      "RoomResponse": {
        "type": "object",
        "required": [
          "bookings",
          "room"
        ],
        "properties": {
          "room": {
            "$ref": "#/components/schemas/Room"
          },
          "bookings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Booking"
            }
          }
        }
      },
      "Booking": {
        "type": "object",
        "required": [
          "date",
          "duration",
          "fk_room_id",
          "fk_user_id",
          "reason",
          "status"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "reason": {
            "type": "string"
          },
          "duration": {
            "description": "0 = Morning, 1 = Afternoon, 2 = Day",
            "type": "integer",
            "format": "int32"
          },
          "status": {
            "description": "Pending, Rejected, Approved",
            "type": "string"
          },
          "date": {
            "description": "YYYY-MM-DD",
            "type": "string"
          },
          "fk_room_id": {
            "type": "integer",
            "format": "int32"
          },
          "fk_user_id": {
            "type": "integer",
            "format": "int32"
          },
          "created_at": {
            "type": "string",
            "nullable": true
          }
        }
      }
    }
  }
}